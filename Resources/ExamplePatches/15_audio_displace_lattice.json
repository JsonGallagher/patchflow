      "params": {
        "fragmentShader": "uniform float u_time;\nuniform vec2 u_resolution;\nuniform float u_param1;\nuniform float u_param2;\nuniform float u_param3;\nuniform float u_param4;\nuniform sampler2D u_spectrum;\n\nfloat hash(vec2 p){\n    return fract(sin(dot(p, vec2(127.1, 311.7))) * 43758.5453);\n}\n\nvoid main() {\n    vec2 uv = gl_FragCoord.xy / u_resolution;\n    vec2 p = uv * 2.0 - 1.0;\n    p.x *= u_resolution.x / u_resolution.y;\n\n    float env = clamp(u_param1, 0.0, 1.0);\n    float high = clamp(u_param2, 0.0, 1.0);\n\n    vec2 grid = p * (12.0 + env * 18.0);\n    vec2 id = floor(grid);\n    vec2 f = fract(grid) - 0.5;\n\n    float rnd = hash(id);\n    float ring = smoothstep(0.42, 0.12, length(f + vec2(sin(u_time + rnd * 6.28) * 0.12, cos(u_time * 0.7 + rnd * 4.0) * 0.12)));\n\n    float spec = texture2D(u_spectrum, vec2(fract((id.x + id.y) * 0.03), 0.0)).r;\n    float pulse = 0.35 + spec * 4.0 + high * 1.2;\n\n    vec3 col = vec3(0.5 + rnd * 0.5, 0.2 + high * 0.8, 0.8) * ring * pulse;\n    col *= 0.5 + env * 0.8;\n\n    gl_FragColor = vec4(clamp(col, 0.0, 1.0), 1.0);\n}\n"
      }
